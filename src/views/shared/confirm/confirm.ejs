<%-contentFor("content")%>
<script src="/javascripts/shared/confirm/confirm-page.js"></script>


<div class="container" style="width:500px;" ng-app="confirm" ng-controller="confirmController">
    <h3 class="page-title text-success">
        <%=title%>
    </h3>

    <div style="font-size: 1.3em;" class="form-group">
        <%=message%>
    </div>

    <div class="form-group">
        <div class="col-md-offset-2">
            <a class="btn btn-sm btn-success" href="<%=closeUrl || 'javascript:window.history.back()' %>"> Close </a>
        </div>
    </div>
</div>